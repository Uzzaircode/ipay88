{
    "name": "uzzaircode/ipay88",
    "description": "laravel package for ipay88",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Uzzair Baharudin",
            "email": "uzzaircode@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Uzzaircode\\Ipay88\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uzzaircode\\Ipay88\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uzzaircode\\Ipay88\\Providers\\Ipay88ServiceProvider"
            ],
            "aliases":{
                "Ipay88":"Uzzaircode\\Ipay88\\Facade\\Ipay88Facade"
            }
        }        
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.8"
    }
}
